{
	"name": "@toreda/template-typescript-cli-app",
	"version": "0.0.0",
	"description": "Complete project demo of a TypeScript CLI app.",
	"main": "./dist/index.js",
	"scripts": {
		"build": "yarn build:prod",
		"build:prod": "yarn gulp --env prod",
		"build:dev": "yarn gulp --env dev",
		"test": "yarn jest --coverage",
		"start": "yarn build & node ./dist/template-app.min.js",
		"lint": "yarn eslint ./src/**.ts --quiet",
		"lint:fix": "yarn eslint ./src/**.ts --quiet --fix",
		"make:docs": "yarn typedoc docs --categorizeByGroup false --entryPoints ./src/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/toreda/template-typescript-cli-app.git"
	},
	"keywords": [
		"demo",
		"app",
		"cli",
		"app",
		"command line app",
		"typescript",
		"project template",
		"toreda"
	],
	"author": "Toreda, Inc.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/toreda/template-typescript-cli-app/issues"
	},
	"homepage": "https://github.com/toreda/template-typescript-cli-app#readme",
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/preset-env": "^7.16.7",

		"@types/node": "^17.0.5",
		"@types/webpack": "^5.28.0",
		"@types/webpack-node-externals": "^2.5.3",
		"@types/yargs": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"babel-loader": "^8.2.3",
		"babel-plugin-const-enum": "^1.2.0",
		"cache-loader": "^4.1.0",
		"core-js": "^3.20.1",
